    <modules>
        <module>post</module>
        <module>comment</module>
        <module>like</module>
        <module>gpt</module>
        <module>user</module>
        <module>search</module>
        <module>forum</module>
        <module>rag</module>
    </modules>
    <artifactId>service</artifactId>
    <version>0.0.1-SNAPSHOT</version>
    <packaging>pom</packaging>
    <name>service</name>
    <description>service</description>
    <url/>
